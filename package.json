    "land-registry-elements": "git+https://github.com/LandRegistry/land-registry-elements.git#v2.1.14"
  },
  "devDependencies": {
    "semver": "^5.3.0",
    "gulp": "^3.9.1",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
