    "prebuild": "npm run check-node-version --silent",
    "preinstall": "./preinstall.sh"
  },
  "engines": {
    "npm": "5.x",
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git@192.168.249.38:skeletons/flask-skeleton-ui.git"
  },
  "dependencies": {
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.3.0",
    "govuk_template_jinja": "0.23.0",
