{
  "name": "flask-skeleton-ui",
  "version": "1.0.0",
  "description": "This repository contains a flask application structured in the way that all Land Registry flask user interfaces should be structured going forwards.",
  "scripts": {
    "check-node-version": "node check-node-version.js",
    "test": "gulp test",
    "build": "gulp clean && gulp copy && gulp build",
    "dev": "npm run build && gulp watch",
    "pretest": "npm run check-node-version --silent",
    "prebuild": "npm run check-node-version --silent"
  },
  "engines": {
    "npm": "5.x",
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git@192.168.249.38:skeletons/flask-skeleton-ui.git"
  },
  "dependencies": {
    "govuk-elements-sass": "3.1.2",
    "govuk_frontend_toolkit": "7.1.0",
    "govuk_template_jinja": "0.23.0",
    "jquery": "1.12.4",
    "land-registry-elements": "git+https://github.com/LandRegistry/land-registry-elements.git#v2.1.13"
  },
  "devDependencies": {
    "semver": "^5.3.0",
    "gulp": "^3.9.1",
    "land-registry-gulp-tasks": "git+https://github.com/LandRegistry/land-registry-gulp-tasks.git#v2.2.0"
  }
}
